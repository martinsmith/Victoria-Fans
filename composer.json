{
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "php": "^8.0.2",
    "craftcms/ckeditor": "^3.15",
    "craftcms/cms": "^4.0",
    "verbb/super-table": "^3.0",
    "verbb/tablemaker": "^4.0",
    "vlucas/phpdotenv": "^5.4"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "allow-plugins": {
      "craftcms/plugin-installer": true,
      "yiisoft/yii2-composer": true
    },
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "8.2"
    }
  }
}
